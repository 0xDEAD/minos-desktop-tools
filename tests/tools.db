@begin{gifcast}
gifcast cui; test X"${?}"                    = X"1"
printf "%s" 'cui' | gifcast; test X"${?}"    = X"1"
gifcast -h;  test X"${?}"                    = X"0"
printf "%s" '-h' | gifcast; test X"${?}"     = X"0"
gifcast --help;  test X"${?}"                = X"0"
printf "%s" '--help' | gifcast; test X"${?}" = X"0"
gifcast --cui;  test X"${?}"                 = X"1"
printf "%s" '--cui' | gifcast; test X"${?}"  = X"1"
test X"$(gifcast cui | head -1)"             = X""
test X"$(gifcast cui 2>&1|head -1)"          = X"Usage: gifcast [options] file.gif"
test X"$(gifcast -h |head -1)"               = X"Usage: gifcast [options] file.gif"
test X"$(gifcast --help | head -1)"          = X"Usage: gifcast [options] file.gif"
test X"$(gifcast --cui  | head -1)"          = X""
