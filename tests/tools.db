touch cui
test X"$(vw cui 2>&1|head -1)"   = X"Error: 'cui' is not a valid image file"
@end

@begin{watch-battery}
watch-battery -h ; test X"${?}"             = X"0"
watch-battery --help ; test X"${?}"         = X"0"
watch-battery --cui; test X"${?}"           = X"1"
test X"$(watch-battery -h |head -1)"        = X"Usage: watch-battery"
test X"$(watch-battery --help | head -1)"   = X"Usage: watch-battery"
test X"$(watch-battery --cui  | head -1)"   = X""
test X"$(watch-battery --cui 2>&1|head -1)" = X"watch-battery: unrecognized option '--cui'"
@end

